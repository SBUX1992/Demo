<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 강중현 -->
<mapper namespace="kr.co.login.dao.MemberDAO">
	<insert id="insertMember">
		insert into `member` set
			`uid`=#{uid},
			`pass`=#{pass},
			`name`=#{name},
			`hp`=#{hp},
			`zip`=#{zip},
			`addr1`=#{addr1},
			`addr2`=#{addr2},
			`regip`=#{regip},
			`rdate`=NOW();
	</insert>








</mapper>